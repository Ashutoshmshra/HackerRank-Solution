import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int result=0;
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        if(N>=2 && N<=20)
            {
            for(int i=1;i<=10;i++)
              // N*i=result; 
            System.out.println(N+" x "+i+" = "+(N*i));
        }
        
    }
}
